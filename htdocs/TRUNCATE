11-05-2023

ALTER TABLE `device_access_control` ADD `register_no` VARCHAR(255) NOT NULL AFTER `upgrade`;
ALTER TABLE `attendance` ADD `device_uniqueid` VARCHAR(255) NOT NULL AFTER `gender`;
ALTER TABLE `attendance` DROP INDEX `p_out_six_time`;
