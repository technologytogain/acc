ALTER TABLE `device_access_control` ADD `academic_year` INT NOT NULL AFTER `register_no`;
ALTER TABLE `device_access_control` CHANGE `academic_year` `academic_year` INT(11) NOT NULL DEFAULT '0';
ALTER TABLE `attendance` ADD `academic_year` INT NOT NULL DEFAULT '0' AFTER `device_uniqueid`;
ALTER TABLE `marks` ADD `academic_year` INT NOT NULL DEFAULT '0' AFTER `sent_at`;
ALTER TABLE `marks` ADD `device_uniqueid` VARCHAR(255) NULL AFTER `academic_year`;
ALTER TABLE `logs` ADD `academic_year` INT NOT NULL DEFAULT '0' AFTER `upgrade`;
ALTER TABLE `notification` ADD `academic_year` INT NOT NULL DEFAULT '0' AFTER `year`;
ALTER TABLE `student_photos` CHANGE `register_no` `unique_id` VARCHAR(255) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL DEFAULT NULL;
